{
    "name": "pravnkay/razorvel",
    "description": "A Razorpay API integration for ease of use in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Praveen K",
            "email": "github@praveen.bulc.club"
        }
    ],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
        "razorpay/razorpay": "2.*"
    },
	"autoload": {
		"psr-4": {
			"Pravnkay\\Razorvel\\": "src"
		}
	},
    "require-dev": {
        "orchestra/testbench": "6.x"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pravnkay\\Razorvel\\Providers\\RazorvelServiceProvider"
            ],
            "aliases": {
				"Razorpay": "Pravnkay\\Razorvel\\Razorpay"
			}
        }
    }
}
